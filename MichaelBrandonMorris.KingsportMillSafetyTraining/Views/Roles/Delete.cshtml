@model MichaelBrandonMorris.KingsportMillSafetyTraining.Models.Data.Role

@{
    ViewBag.Title = "Delete";
    ViewBag.Subtitle = "Are you sure you want to delete this?";
}

@Html.DisplayForModel(Model)

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div>
        <input class="btn btn-default"
               type="submit"
               value="Delete"/> |
        <button class="btn btn-default"
                onclick="location.href = '@Url.Action("Index")';return false;">
            Cancel
        </button>
    </div>
}