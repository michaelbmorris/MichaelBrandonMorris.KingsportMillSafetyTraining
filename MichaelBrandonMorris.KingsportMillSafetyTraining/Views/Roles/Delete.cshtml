@model MichaelBrandonMorris.KingsportMillSafetyTraining.Models.RoleViewModel

@{
    ViewBag.Title = "Delete";
    ViewBag.Subtitle = "Are you sure you want to delete this?";
}

@Html.DisplayForModel(Model)

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div>
        <input class="btn btn-danger"
               type="submit"
               value="Delete"/>
        @Html.ActionLink("Cancel", "Index", null, new
        {
            @class = "btn btn-primary"
        })
    </div>
}